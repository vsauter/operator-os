{
  "name": "@operator/connectors",
  "version": "0.1.0",
  "private": true,
  "description": "MCP connectors for various data sources (Gong, Pylon, etc.)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./gong": {
      "types": "./dist/gong/index.d.ts",
      "import": "./dist/gong/index.js"
    },
    "./pylon": {
      "types": "./dist/pylon/index.d.ts",
      "import": "./dist/pylon/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3"
  }
}
